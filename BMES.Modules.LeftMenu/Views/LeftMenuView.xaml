<UserControl
    x:Class="BMES.Modules.LeftMenu.Views.LeftMenuView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:BMES.Modules.LeftMenu.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    d:DesignHeight="800"
    d:DesignWidth="50"
    prism:ViewModelLocator.AutoWireViewModel="True"
    mc:Ignorable="d">
    <Grid Background="#23272A">
        <Grid.RowDefinitions>
           
            <RowDefinition Height="31"/>
            <RowDefinition Height="31"/>
            <RowDefinition Height="31"/>
            <RowDefinition Height="31"/>
            <RowDefinition Height="31"/>
            <RowDefinition Height="31"/>
        </Grid.RowDefinitions>
        
        <Grid Grid.Row="0">
            <Button Margin="1 2"/>
        </Grid>
        <Grid Grid.Row="1">
            <Button Margin="1 2"/>
        </Grid>
        <Grid Grid.Row="2">
            <Button Margin="1 2"/>
        </Grid>
        <Grid Grid.Row="3">
            <Button Margin="1 2"/>
        </Grid>
        <Grid Grid.Row="4">
            <Button Margin="1 2"/>
        </Grid>

    </Grid>
</UserControl>
